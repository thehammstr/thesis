@InProceedings{Souibgui2011,
  Title                    = {Particle filter approach to fault detection and isolation in nonlinear systems},
  Author                   = {Souibgui, F. and BenHmida, F. and Chaari, A.},
  Booktitle                = {Systems, Signals and Devices (SSD), 2011 8th International Multi-Conference on},
  Year                     = {2011},
  Month                    = {march},
  Pages                    = {1 -6},

  Abstract                 = {This paper introduces the particle-filtering (PF) based framework for fault diagnosis in non-linear systems and noise and disturbances being Gaussian. In this paper, we use the sequential Monte Carlo filtering approach where the complete posterior distribution of the estimates are represented through samples or particles as opposed to the mean and covariance of an approximated Gaussian distribution. We compare the fault detection performance with that using the extended Kalman filtering and investigate the isolation performance on a nonlinear system.},

@InProceedings{Hagen2007,
  Title                    = {Making {AUV}s Truly Autonomous},
  Author                   = {Hagen, P.E. and Midtgaard, O. and Hasvold, O.},
  Booktitle                = {OCEANS 2007},
  Year                     = {2007},

@InProceedings{Hagen2013,
  Title                    = {Robust surface vessel navigation using terrain navigation},
  Author                   = {Hagen, O.K. and Anonsen, K.B. and Skaugen, A.},
  Booktitle                = {OCEANS - Bergen, 2013 MTS/IEEE},
  Year                     = {2013},

@Article{Silvestre2009,
  Title                    = {A bottom-following preview controller for autonomous underwater vehicles},
  Author                   = {Silvestre, Carlos and Cunha, Rita and Paulino, Nuno and Pascoal, Ant{\'o}nio},
  Journal                  = {Control Systems Technology, IEEE Transactions on},
  Year                     = {2009},
  Number                   = {2},
  Pages                    = {257--266},
  Volume                   = {17},

  Abstract                 = {This paper describes a solution to the problem of bottom-following for autonomous underwater vehicles (AUVs) that relies on the evaluation of the terrain characteristics ahead of the vehicle using echo sounders. The methodology used poses the problem as a discrete time path following control problem where a conveniently defined error state space model of the plant is augmented with bathymetric (i.e., depth) preview data. A piecewise affine parameter-dependent model representation is adopted that describes the AUV linearized error dynamics for a predefined set of operating regions. For each region, a state feedback H 2 control problem for affine parameter-dependent systems is posed and solved using linear matrix inequalities (LMIs). The resulting nonlinear controller is implemented as a gain-scheduled controller using the D-methodology. Simulation results obtained with a nonlinear dynamic model of the INFANTE AUV in the vertical plane are presented and discussed.},
  File                     = {Silvestre2009.pdf:Silvestre2009.pdf:PDF},
  Publisher                = {IEEE}
}

@InProceedings{Sim2005,
  Title                    = {Global A-Optimal Robot Exploration in {SLAM}},
  Author                   = {Sim, R. and Roy, N.},
  Booktitle                = {Robotics and Automation, 2005. ICRA 2005. Proceedings of the 2005 IEEE International Conference on},
  Year                     = {2005},
  Month                    = april,
  Pages                    = { 661 - 666},

  Abstract                 = {It is well-known that the Kalman filter for simultaneous localization and mapping (SLAM) converges to a fully correlated map in the limit of infinite time and data [1]. However, the rate of convergence of the map has a strong dependence on the order of the observations. We show that conventional exploration algorithms for collecting map data are sub-optimal in both the objective function and choice of optimization procedure. We show that optimizing the a-optimal information measure results in a more accurate map than existing approaches, using a greedy, closed-loop strategy. Secondly, we demonstrate that by restricting the planning to an appropriate policy class, we can tractably find non-greedy, global planning trajectories that produce more accurate maps, explicitly planning to close loops even in open-loop scenarios.},
  Doi                      = {10.1109/ROBOT.2005.1570193},
  File                     = {Sim2005.pdf:Sim2005.pdf:PDF}
}

@Article{Singh2004,
  Title                    = {Imaging Coral I: Imaging Coral Habitats with the SeaBED AUV},
  Author                   = {Singh, Hanumant and Armstrong, Roy and Gilbes, Fernando and Eustice, Ryan and Roman, Chris and Pizarro, Oscar and Torres, Juan},

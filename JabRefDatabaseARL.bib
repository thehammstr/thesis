  Author                   = {Hahnel, D. and Burgard, W. and Fox, D. and Thrun, S.},
  Booktitle                = {Intelligent Robots and Systems, 2003. (IROS 2003). Proceedings. 2003 IEEE/RSJ International Conference on},

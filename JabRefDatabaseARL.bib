  Title                    = {Contour Image Match and Its Application in Underwater Navigation and Positioning},
  Author                   = {Jianhu Zhao and Xiankuo Meng and Nan Shao and Hongmei Zhang},
  Booktitle                = {Computational Intelligence and Software Engineering, 2009. CiSE 2009. International Conference on},

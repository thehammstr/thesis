@InProceedings{Kanjanawanishkul2009,
  Title                    = {Smooth Reference Tracking of a Mobile Robot using Nonlinear Model Predictive Control.},
  Author                   = {Kanjanawanishkul, Kiattisin and Hofmeister, Marius and Zell, Andreas},
  Booktitle                = {ECMR},
